{
    "name": "helturkey/bidi",
    "description": "",
    "keywords": [
        "bidi"
    ],
    "homepage": "https://github.com/helturkey/bidi",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hussein",
            "email": "hh@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.3|^8.0"
    },
    "autoload": {
        "psr-0": {
            "Hussein\\BiDi\\ArabicUtf8": "src/"
        },
        "psr-4": {
            "Hussein\\BiDi\\ArabicUtf8\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
